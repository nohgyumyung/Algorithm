   "source": [
    "n = int(input())\n",
    "\n",
    "grades = []\n",
    "for i in range(n):\n",
    "    name, kor, eng, math = input().split(' ')\n",
    "    grades.append((name, int(kor), int(eng), int(math)))\n",
    "\n",
    "grades.sort(key=lambda x:(-x[1], x[2], -x[3], x[0]))\n",
    "\n",
    "for i in grades:\n",
    "    print(i[0])"
   ]
  }
